<section ng-app>
    <section class="container-fluid animated fadeInDown delay-1">
            <div class="col-sm-5 col-sm-offset-4 col-md-5 col-md-offset-5 col-lg-offset-7 col-lg-3 borde-login">
                <div class="col-lg-12">
                {{#if success_msg}}
                  <div class="alert alert-success">{{success_msg}}</div>
                {{/if}}

                {{#if error_msg}}
                  <div class="alert alert-danger">{{error_msg}}</div> 
                {{/if}}

                {{#if error}}
                  <div class="alert alert-danger">{{error}}</div>
                {{/if}}

            </div>
                <form name="form2" ng-submit="vm.login()" role="form" method="post" action="/login">
                    <div class="col-sm-11 col-md-12 col-lg-12 form-group" ng-class="{ 'has-error': form2.username.$dirty && form2.username.$error.required }">
                        <label class="sr-only" for="username">Correo</label>
                        <input placeholder="Correo electrónico" type="mail" name="username" id="username" class="form-control" ng-model="vm.username" required />
                        <span ng-show="form2.username.$dirty && form2.username.$error.required" class="help-block">Escriba su correo</span>
                    </div>
                    <div class="col-sm-7 col-md-7 col-lg-7 form-group" ng-class="{ 'has-error': form2.password.$dirty && form2.password.$error.required }">
                        <label class="sr-only" for="password">Contraseña</label>
                        <input placeholder="Contraseña" type="password" name="password" id="password" class="form-control" ng-model="vm.password" required />
                        <span ng-show="form2.password.$dirty && form2.password.$error.required" class="help-block">Escriba su contraseña</span>
                    </div>
                    <div class="col-sm-3 col-md-2 col-lg-3 form-actions">
                        <button type="submit" ng-disabled="form2.$invalid || vm.dataLoading" class="btn btn-primary">Iniciar sesión </button>
                        <img ng-if="vm.dataLoading" src="data:image/gif;base64," />
                    </div>
                </form>
            </div>
    </section>
    <section class="animated fadeInDown delay-2">

         <div class="col-sm-5 col-sm-offset-4 col-md-5 col-md-offset-5 col-lg-offset-7 col-lg-3 borde-login caja-rg">
            <h2>Registrate</h2>
            {{#if errors}}
                {{#each errors}}
                    <div class="alert alert-danger">{{msg}}</div>
                {{/each}}
            {{/if}}
            <form name="form" ng-submit="vm.register()" role="form" method="post" action="/">
                <div class="form-group" ng-class="{ 'has-error': form.name.$dirty && form.name.$error.required }">
                    <label class="sr-only" for="username">Nombre</label>
                    <input placeholder="Nombre" type="text" name="name" id="name" class="form-control" ng-model="vm.user.name" required />
                    <span ng-show="form.name.$dirty && form.name.$error.required" class="help-block">Escriba su nombre</span>
                </div>
                <div class="form-group" ng-class="{ 'has-error': form.email.$dirty && form.email.$error.required }">
                    <label class="sr-only" for="email">Email</label>
                    <input placeholder="Correo electrónico"  type="text" name="email" id="Text1" class="form-control" ng-model="vm.user.email" required />
                    <span ng-show="form.email.$dirty && form.email.$error.required" class="help-block">Escriba su correo ejemplo: usuario@hotmail.com</span>
                </div>
                <div class="form-group" ng-class="{ 'has-error': form.password.$dirty && form.password.$error.required }">
                    <label class="sr-only" for="password">Contraseña</label>
                    <input placeholder="contraseña" type="password" name="password" id="password" class="form-control" ng-model="vm.user.password" required />
                    <span ng-show="form.password.$dirty && form.password.$error.required" class="help-block">Escriba la contraseña</span>
                </div>
                <div class="form-group" ng-class="{ 'has-error': form.password.$dirty && form.password.$error.required }">
                    <label class="sr-only" for="password">Confirmar Contraseña</label>
                    <input placeholder="Repita la contraseña" type="password" name="password2" id="password" class="form-control" ng-model="vm.user.password2" required />
                    <span ng-show="form.password.$dirty && form.password.$error.required" class="help-block">Repita la contraseña que  escribió</span>
                </div>
                <div class="form-actions">
                    <button type="submit" ng-disabled="form.$invalid || vm.dataLoading" class="btn btn-success">Registrarse</button>
                    <img ng-if="vm.dataLoading" src="data:image/gif;base64," />

                </div>
            </form>
        </div>
    </section>
</section>
